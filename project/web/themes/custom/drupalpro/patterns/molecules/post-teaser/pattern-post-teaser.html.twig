<div class="card card__post_teaser">
    <div class="card__title">
        <h5> {{ title }}</h5>
    </div>
    <p class="card__text">{{ text }}</p>
    <div class="card__image">
        <div class="image">{{ image }}</div>
    </div>
    <p class="card__text_category">Category:</p>
    <div class="card__button_category">{{ category_button }}</div>
</div>